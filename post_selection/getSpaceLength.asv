function out_tab = getSpaceLength(in_arr,box)
% GET SPACE LENGTH 
% From a word, get the # of pixels for each space

wList = in_arr; % list of words
spaceSize = [];

for k = 1:size(wList,1)
    
    wLength = wList{k,4}; % length of this word - only chars 
    
    spaceSize(k) = round((box.max_absolute - wLength) / 5);
    
end

out_tab = spaceSize';
end

